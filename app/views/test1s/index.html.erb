<p id="notice"><%= notice %></p>

<h1>Test1s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test1s.each do |test1| %>
      <tr>
        <td><%= test1.Name %></td>
        <td><%= test1.Age %></td>
        <td><%= link_to 'Show', test1 %></td>
        <td><%= link_to 'Edit', edit_test1_path(test1) %></td>
        <td><%= link_to 'Destroy', test1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test1', new_test1_path %>
